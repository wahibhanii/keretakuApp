<!DOCTYPE html>
<html>

  <body>
    <h1>Users List</h1>
    <h3><a href="/">Return to Home</a></h3>
    <table border="1">
      <thead>
        <tr>
          <th>Id</th>
          <th>E-mail</th>
          <th>Password</th>
          <th>Role</th>
          <th>Point</th>
        </tr>
      </thead>
      <% dataUsers.forEach((user)=>{ %>
        <tr>
            <td><%=user.id%></td>
            <td><%=user.email%></td>
            <td><%=user.password%></td>
            <td><%=user.role%></td>
            <td><%=user.point%></td>
        </tr>
      <%})%>
    </table>
  </body>

</html>